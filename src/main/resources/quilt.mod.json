{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.pepe20129",
		"id": "splashesplus",
		"version": "${version}",
		"entrypoints": {
			"client_init": "io.github.pepe20129.splashes.QuiltSplashesPlus"
		},
		"depends": [
			{
				"id": "java",
				"versions": ">=17"
			},
			{
				"id": "minecraft",
				"versions": "1.19.x"
			},
			{
				"id": "quilt_loader",
				"versions": ">=0.16.0"
			}
		],
		"intermediate_mappings": "net.fabricmc:intermediary",
