			"contributors": {
				"Pepe20129/Pablo#1981": "Author"
			},
			"contact": {
				"homepage": "https://github.com/Pepe20129/splashesplus",
				"issues": "https://github.com/Pepe20129/splashesplus/issues",
				"sources": "https://github.com/Pepe20129/splashesplus"
			},
			"license": "LGPL-3.0-only",
			"icon": "assets/splashesplus/icon.png"
		}
	},
	"mixin": "splashes.mixins.json",
	"minecraft": {
		"environment": "client"
	}
}